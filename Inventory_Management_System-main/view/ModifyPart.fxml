            <RadioButton fx:id="addPartOutsourcedRadio" layoutX="387.0" layoutY="53.0" mnemonicParsing="false" onAction="#addPartOutsourcedRadioAction" text="Outsourced" toggleGroup="$modifyPartToggleGroup" />
