            <TextField fx:id="addPartInvText" layoutX="196.0" layoutY="187.0" />
            <TextField fx:id="addPartPriceText" layoutX="196.0" layoutY="217.0" />
            <TextField fx:id="addPartMaxText" layoutX="196.0" layoutY="247.0" />
            <TextField fx:id="addPartMachineIDText" layoutX="196.0" layoutY="277.0" />
            <Label layoutX="373.0" layoutY="251.0" text="Min" />
            <TextField fx:id="addPartMinText" layoutX="435.0" layoutY="247.0" />
         </children>
      </AnchorPane>
   </children>
